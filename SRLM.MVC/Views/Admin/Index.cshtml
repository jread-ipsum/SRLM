@model SRLM.Models.AdminDashboardModels.AdminDashboard
@{
    ViewBag.Title = "Index";
}

<br />

<div class="row row-cols-1 row-cols-md-2 g-4">
    <div class="col">
      @Html.Partial("_PlatformIndex", Model.PlatformListItems)
    </div>
    <div class="col">
        @Html.Partial("_TrackIndex", Model.TrackListItems)
    </div>
    <div class="col">
        <div class="card">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Game Manager</h5>
                <p class="card-text">This is a longer card with supporting text below as a natural lead-in to additional content.</p>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">Car Manager</h5>
                <p class="card-text">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
            </div>
        </div>
    </div>
    <div class="col">
        <div class="card">
            <img src="..." class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">League Manager</h5>
                <p class="card-text">This is a longer card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
            </div>
        </div>
    </div>
    <div class="col">
       @Html.Partial("_RaceClassIndex", Model.RaceClassListItems)
    </div>
</div>

